{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "bun lint:eslint && bun lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint:typecheck": "nuxi typecheck",
    "lintfull": "bun lint && bun lint:typecheck",
    "lintfix": "eslint . --fix && prettier --write --list-different ."
  },
  "dependencies": {
    "@nuxt/eslint": "1.3.1",
    "@tailwindcss/vite": "^4.1.7",
    "eslint": "^9.27.0",
    "nuxt": "^3.17.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vue": "^3.5.15",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.0"
  },
  "devDependencies": {
    "@pinia/nuxt": "^0.11.0",
    "@vueuse/nuxt": "^13.3.0",
    "ofetch": "^1.4.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "vue-tsc": "^2.2.10"
  }
}
